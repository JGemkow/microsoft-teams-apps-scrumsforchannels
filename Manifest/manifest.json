{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "version": "1.0.0",
  "id": "c683b4d3-97de-4331-8340-63fcd93b47e6",
  "packageName": "com.microsoft.teams.scrumsforchannels",
  "developer": {
    "name": "<<companyName>>",
    "websiteUrl": "<<websiteUrl>>",
    "privacyUrl": "<<privacyUrl>>",
    "termsOfUseUrl": "<<termsOfUseUrl>>"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Scrums for channels"
  },
  "description": {
    "short": "Scrums for channels helps you get status updates from your team in channel scope",
    "full": "Scrums for channels is a simple scrum assistant that will enable users to run asynchronous stand-up meetings and provide an easy way for the users to share their daily updates."
  },
  "accentColor": "#FFFFFF",
  "bots": [
    {
      "botId": "<<botId>>",
      "scopes": [
        "team"
      ],
      "commandLists": [
        {
          "scopes": [
            "team"
          ],
          "commands": [
            {
              "title": "Open settings",
              "description": "Open settings to add or update scrums"
            },
            {
              "title": "Help",
              "description": "Shows the operations I can perform"
            }
          ]
        }
      ],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "<<valid domains>>"
  ]
}